<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_full_scream"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/verde"
    android:gravity="center"
    android:orientation="vertical"
    android:paddingBottom="@dimen/padding_botton_fullScreem"
    android:paddingLeft="@dimen/padding_left_fullScreen"
    android:paddingRight="@dimen/padding_right_fullScreem"
    android:paddingTop="@dimen/padding_top_fullScreem"
    tools:context="com.example.renatogallis.fixcard.FullScreanActivity">

    <ImageView
        android:id="@+id/splash"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/transparent" />

</LinearLayout>
